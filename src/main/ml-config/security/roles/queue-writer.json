{
  "role-name": "hub-queue-writer",
  "description": "Role that has write permission for queue documents",
  "privilege": [
    {
      "privilege-name": "admin-app-server",
      "action": "http://marklogic.com/xdmp/privileges/admin/app-server",
      "kind": "execute"
    },
    {
      "privilege-name": "xdmp:invoke",
      "action": "http://marklogic.com/xdmp/privileges/xdmp-invoke",
      "kind": "execute"
    },
    {
      "privilege-name": "queue-writer",
      "action": "http://noslogan.org/hub-queue/queue-writer",
      "kind": "execute"
    },
    {
      "privilege-name": "xdmp:invoke-in",
      "action": "http://marklogic.com/xdmp/privileges/xdmp-invoke-in",
      "kind": "execute"
    },
    {
      "privilege-name": "queue-event-privilege",
      "action": "%%comQueuePrefix%%",
      "kind": "uri"
    },
    {
      "privilege-name": "queue-log-privilege",
      "action": "%%comQueuePrefix%%log/",
      "kind": "uri"
    }
  ]
}